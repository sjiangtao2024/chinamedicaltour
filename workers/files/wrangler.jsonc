{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "files",
  "main": "src/index.js",
  "compatibility_date": "2025-12-24",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true
  },
  "vars": {},
  "r2_buckets": [
    {
      "binding": "FILES_BUCKET",
      "bucket_name": "cmt-files"
    }
  ],
  "routes": [
    {
      "pattern": "files.chinamedicaltour.org/*",
      "zone_name": "chinamedicaltour.org"
    }
  ]
}
